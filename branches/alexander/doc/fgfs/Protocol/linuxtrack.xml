<?xml version="1.0"?>

<PropertyList>

	<comment>
		<![CDATA[
    		Usage:
					fgfs --generic=socket,in,100,,6543,udp,linuxtrack
		]]>
	</comment>

	<generic>

		<input>

			<line_separator>newline</line_separator>
			<var_separator>tab</var_separator>

			<chunk>
				<name>heading-deg</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/heading</node>
			</chunk>
   
			<chunk>
				<name>pitch-deg</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/pitch</node>
			</chunk>

			<chunk>
				<name>roll-deg</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/roll</node>
			</chunk>

			<chunk>
				<name>postion-x</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/tx</node>
			</chunk>

			<chunk>
				<name>position-y</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/ty</node>
			</chunk>

			<chunk>
				<name>position-z</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/tz</node>
			</chunk>

		</input>
 
	</generic>

</PropertyList>
